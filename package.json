{
  "name": "online-store",
  "version": "1.0.0",
  "description": "online-store",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "start": "webpack serve --mode=development --node-env=development",
    "pretty": "pretty-quick --staged && lint-staged",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lintfix": "npm run lint -- --fix",
    "pre": "prettier --write \"src/**/*.{js,jsx,scss,css,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,scss,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bp26/online-store.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bp26/online-store/issues"
  },
  "homepage": "https://github.com/bp26/online-store#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@webpack-cli/generators": "^3.0.1",
    "autoprefixer": "^10.4.12",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
